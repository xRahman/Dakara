  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <Content> si vybere </Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Italic">
    <Content>starotrpasličtinu</Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <Content> jako svůj druhý starý jazyk a k tomu </Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Italic">
    <Content>přetvářku a převleky</Content>
